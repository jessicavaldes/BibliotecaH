    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InicioSes</servlet-name>
        <url-pattern>/InicioSes</url-pattern>
